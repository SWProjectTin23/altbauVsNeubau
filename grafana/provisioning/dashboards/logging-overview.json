  "links": [],
  "liveNow": false,
  "panels": [
    {
      "type": "logs",
      "title": "Live Logs",
      "id": 1,
      "gridPos": {"h": 10, "w": 24, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "{env=~\"$env\",service=~\"$service\",compose_service=~\"$compose_service\",level=~\"$level\"}",
          "queryType": "range",
          "refId": "A"
        }
      ],
      "options": {
        "dedupStrategy": "none",
        "enableLogDetails": true,
        "showLabels": true,
        "showCommonLabels": true,
        "wrapLogMessage": true
      }
    },
    {
      "type": "timeseries",
      "title": "Error Rate (5m) by service",
      "id": 2,
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 10},
      "targets": [
        {
          "expr": "sum by(service) (count_over_time({env=~\"$env\", level=\"ERROR\", service=~\"$service\"}[5m]))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Requests/min (API) â€“ event=request_ok",
      "id": 3,
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 10},
      "targets": [
        {
          "expr": "sum by(service) (count_over_time({env=~\"$env\", service=~\"$service\", event=\"request_ok\"}[1m]))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "API P95 duration_ms (5m window)",
      "id": 4,
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 18},
      "targets": [
        {
          "expr": "quantile_over_time(0.95, ({env=~\"$env\", service=~\"$service\", event=\"request_ok\"} | json | unwrap duration_ms)[5m])",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "MQTT processed/min by metric",
      "id": 5,
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 18},
      "targets": [
        {
          "expr": "sum by(metric) (count_over_time({env=~\"$env\", service=\"ingestor\", event=\"msg_processed\", metric=~\"$metric\"}[1m]))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "bargauge",
      "title": "Top 5 Warning reasons (last 1h)",
      "id": 6,
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 26},
      "targets": [
        {
          "expr": "topk(5, sum by(reason) (count_over_time({env=~\"$env\", level=\"WARNING\", reason!=\"\"}[1h])))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "bargauge",
      "title": "Top 5 Error modules (last 1h)",
      "id": 7,
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 26},
      "targets": [
        {
          "expr": "topk(5, sum by(module) (count_over_time({env=~\"$env\", level=\"ERROR\", module!=\"\"}[1h])))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "table",
      "title": "Recent Errors",
      "id": 8,
      "gridPos": {"h": 9, "w": 24, "x": 0, "y": 34},
      "targets": [
        {
          "expr": "{env=~\"$env\", level=\"ERROR\", service=~\"$service\"} | json",
          "refId": "A"
        }
      ],
      "options": {
        "showHeader": true
      },
      "transformations": [
        {
          "id": "labelsToFields",
          "options": {
            "mode": "columns"
          }
        },
        {
          "id": "extractFields",
          "options": {
            "format": "json",
            "source": "line"
          }
        }
      ]
    }
  ],
